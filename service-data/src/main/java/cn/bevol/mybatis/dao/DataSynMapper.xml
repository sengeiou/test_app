<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 
	namespace：必须与对应的接口全类名一致
	id:必须与对应接口的某个对应的方法名一致
	
 -->
<mapper namespace="cn.bevol.mybatis.dao.DataSynMapper">

    <delete id="deleteBySql" parameterType="String">
		${sql} 
	</delete>
	
	<insert id="insertBySql" parameterType="String">
		${sql}
 	</insert>
 	
 	 <update id="updateBySql" parameterType="String">
		${sql}
	</update>
	
 	 <select id="selectBySql" parameterType="String"  resultType="Map">
 		${sql}
	</select>
 	
 </mapper>